{
  "name": "deep-shallow",
  "version": "0.1.1",
  "description": "An object convert between shallow and deep",
  "main": "dist/deep-shallow.js",
  "cjs:main": "dist/deep-shallow.js",
  "module": "dist/deep-shallow.m.js",
  "jsnext:main": "dist/deep-shallow.m.js",
  "umd:main": "dist/deep-shallow.umd.js",
  "types": "deep-shallow.d.ts",
  "keywords": [
    "object"
  ],
  "files": [
    "dist",
    "deep-shallow.d.ts",
    "deep-shallow.js.flow"
  ],
  "scripts": {
    "build": "microbundle -i deep-shallow.ts -o dist && (yarn dts > /dev/null 2>&1) && (yarn jsflow > /dev/null 2>&1)",
    "dts": "tsc -p tsconfig.d.json",
    "jsflow": "npx flowgen deep-shallow.d.ts -o deep-shallow.js.flow",
    "dev": "microbundle watch -o dist",
    "test": "jest",
    "test:ci": "jest && cat ./coverage/lcov.info | coveralls"
  },
  "repository": "git@github.com:nju33/deep-shallow.git",
  "author": "nju33 <nju33.ki@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@geekcojp/tslint-config": "^0.1.2",
    "@types/jest": "^23.1.1",
    "coveralls": "^3.0.1",
    "jest": "^23.1.0",
    "microbundle": "^0.4.4",
    "prettier": "^1.13.5",
    "ts-jest": "^22.4.6",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.110",
    "lodash": "^4.17.10"
  },
  "engines": {
    "node": ">=8"
  }
}
