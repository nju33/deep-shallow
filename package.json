{
  "name": "deep-shallow",
  "version": "0.0.1",
  "description": "An object convert between shallow and deep",
  "main": "dist/deep-shallow.m.js",
  "jsnext:main": "dist/deep-shallow.js",
  "module": "dist/deep-shallow.js",
  "types": "deep-shallow.d.ts",
  "keywords": [
    "object"
  ],
  "files": [
    "dist",
    "deep-shallow.d.ts",
    "deep-shallow.js.flow"
  ],
  "scripts": {
    "build": "microbundle -i deep-shallow.ts -o dist && yarn dts | : && yarn jsflow | :",
    "dts": "tsc -p tsconfig.d.json",
    "jsflow": "npx flowgen deep-shallow.d.ts -o deep-shallow.js.flow",
    "dev": "microbundle watch -o dist",
    "test": "jest",
    "test:ci": "jest && cat ./coverage/lcov.info | coveralls"
  },
  "repository": "git@github.com:nju33/deep-shallow.git",
  "author": "nju33 <nju33.ki@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@geekcojp/tslint-config": "^0.1.2",
    "@types/jest": "^22.1.1",
    "coveralls": "^3.0.0",
    "jest": "^22.2.1",
    "microbundle": "^0.4.3",
    "prettier": "^1.10.2",
    "ts-jest": "^22.0.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.102",
    "lodash": "^4.17.5"
  },
  "engines": {
    "node": ">=8"
  }
}
